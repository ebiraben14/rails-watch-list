<div class="container mt-5">
  <div class="row">
    <div class="col">
      <h1>Add a new List</h1>

        <%= simple_form_for [ @list, @bookmark ] do |f| %>
              <%= f.input :comment %>
              <%= f.association :movie %>
              <%= f.submit "Add a Bookmark" %>
              <% end %>
      </div>
   </div>
</div>
